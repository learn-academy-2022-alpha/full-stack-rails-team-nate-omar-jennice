<h2>Create a new blog post</h2>
<%= form_with model: @blog do |form| %>
    <%= form.label :title %>
    <%= form.text_field :title %>
    <br>
    <%= form.label :content %>
    <%= form.text_field :content %>
    <br>
    <%= form.submit 'Add Blog Post' %>
  <% end %>
  <p><%= link_to 'Back to Home Page', blogs_path %></p>